    "historical_context": "\u062f\u064a\u062f\u064b\u0627 \u0648\u0647\u0648 \u0627\u0644\u0623\u0645\u0627\u0646 \u0627\u0644\u0631\u0642\u0645\u064a\u060c \u062d\u064a\u062b \u0633\u0623\u0639\u0645\u0644 \u0639\u0644\u0649 \u062d\u0645\u0627\u064a\u0629 \u0627\u0644\u0646\u0638\u0645 \u0627\u0644\u0631\u0642\u0645\u064a\u0629 \u0645\u0646 \u0627\u0644\u0627\u062e\u062a\u0631\u0627\u0642\u0627\u062a \u0627\u0644\u0623\u0645\u0646\u064a\u0629 \u0648\u0641\u0642\u062f\u0627\u0646 \u0627\u0644\u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u062d\u0633\u0627\u0633\u0629.\n\n**\u062e\u0637\u0629 \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u062a\u062d\u062f\u064a\u062b\u064a\u0629:**\n\n1. \u0633\u0623\u0633\u062a\u062b\u0645\u0631 \u0641\u064a \u062a\u0637\u0648\u064a\u0631 \u062a\u0642\u0646\u064a\u0627\u062a \u0627\u0644\u0630\u0643\u0627\u0621 \u0627\u0644\u0627\u0635\u0637\u0646\u0627\u0639\u064a \u0627\u0644\u0645\u062a\u0642\u062f\u0645\u0629 \u0644\u062a\u062d\u0633\u064a\u0646 \u0627\u0644\u0643\u0641\u0627\u0621\u0629 \u0648\u0627\u0644\u0633\u064a\u0627\u062f\u0629 \u0627\u0644\u0631\u0642\u0645\u064a\u0629.\n2. \u0633\u0623\u0639\u0645\u0644 \u0639\u0644\u0649 \u0625\u0646\u0634\u0627\u0621 \u0623\u0646\u0638\u0645\u0629 \u0631\u0642\u0645\u064a\u0629 \u0645\u0633\u062a\u0642\u0644\u0629 \u0648\u0645\u062d\u0643\u0648\u0645\u0629 \u0628\u0627\u0644\u0630\u0643\u0627\u0621 \u0627\u0644\u0627\u0635\u0637\u0646\u0627\u0639\u064a \u0644\u062a\u062d\u064a\u064a\u062f \u0627\u0644\u0645\u062a\u063a\u064a\u0631\u0627\u062a \u0627\u0644\u0628\u0634\u0631\u064a\u0629.\n3. \u0633\u0623\u0633\u062a\u062b\u0645\u0631 \u0641\u064a \u062a\u0637\u0648\u064a\u0631 \u0623\u0646\u0638\u0645\u0629 \u0623\u0645\u0627\u0646 \u0631\u0642\u0645\u064a \u0645\u062a\u0642\u062f\u0645\u0629 \u0644\u062d\u0645\u0627\u064a\u0629 \u0627\u0644\u0646\u0638\u0645 \u0627\u0644\u0631\u0642\u0645\u064a\u0629 \u0645\u0646 \u0627\u0644\u0627\u062e\u062a\u0631\u0627\u0642\u0627\u062a \u0627\u06