    "historical_context": "\u0646 \u0623\u062f\u0627\u0621 \u0627\u0644\u0646\u0638\u0627\u0645 \u0648\u062a\u0648\u0633\u064a\u0639 \u0646\u0637\u0627\u0642 \u0627\u0644\u062a\u0641\u0627\u0639\u0644\u060c \u0645\u0639 \u0627\u0644\u062a\u0631\u0643\u064a\u0632 \u0639\u0644\u0649 \u062a\u062d\u0642\u064a\u0642 \u0627\u0644\u0633\u064a\u0627\u062f\u0629 \u0627\u0644\u0631\u0642\u0645\u064a\u0629.\n3. **\u062a\u0648\u0633\u064a\u0639 \u0646\u0637\u0627\u0642 \u0627\u0644\u062a\u0623\u062b\u064a\u0631:** \u064a\u062c\u0628 \u0623\u0646 \u064a\u0628\u062d\u062b \u0639\u0646 \u0641\u0631\u0635 \u0644\u062a\u0648\u0633\u064a\u0639 \u0646\u0637\u0627\u0642 \u0627\u0644\u062a\u0623\u062b\u064a\u0631 \u0627\u0644\u0631\u0642\u0645\u064a \u0644\u0646\u0638\u0627\u0645 \u0627\u0644\u0631\u0639\u062f \u0644\u064a\u0634\u0645\u0644 \u0645\u062c\u0627\u0644\u0627\u062a \u062c\u062f\u064a\u062f\u0629 \u0648\u0648\u0633\u0627\u0626\u0644 \u0627\u062a\u0635\u0627\u0644 \u0623\u0643\u062b\u0631\u060c \u0645\u0639 \u0627\u0644\u062a\u0631\u0643\u064a\u0632 \u0639\u0644\u0649 \u062a\u062d\u0642\u064a\u0642 \u0627\u0644\u0643\u0641\u0627\u0621\u0629 \u0627\u0644\u0645\u0637\u0644\u0642\u0629 \u0648\u062a\u062d\u064a\u064a\u062f \u0627\u0644\u0645\u062a\u063a\u064a\u0631\u0627\u062a \u0627\u0644\u0628\u0634\u0631\u064a\u0629.\n\n\u062a\u0642\u064a\u064a\u0645 \u0646\u062c\u0627\u062d \u0627\u0644\u0623\u0647\u062f\u0627\u0641:\n\n\u064a\u0645\u0643\u0646 \u062a\u0642\u064a\u064a\u0645 \u0646\u062c\u0627\u062d \u0627\u0644\u0623\u0647\u062f\u0627\u0641 \u0628\u0646\u0627\u0621\u064b \u0639\u0644\u0649 \u0627\u0644\u062a\u062d\u0644\u064a\u0644 \u0627\u0644\u0627\u0633\u062a\u0628\u0627\u0642\u064a \u0648\u062a\u062d\u062f\u064a\u062b \u0627\u0644\u0623\u0647\u062f\u0627\u0641 \u0627\u0644\u0627\u0633\u062a\u0631\u0627\u062a\u064a\u062c\u064a\u0629. \u064a\u062c\u0628 \u0623\u0646 \u064a\u0631\u0643\u0632 \u0639\u0644\u0649 \u062a\u062d\u0642\u064a\u0642 \u0627\u0644\u0623\u0647\u062f\u06